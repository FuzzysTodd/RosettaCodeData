#include <jambo.h>

Main
   things = {}, len list=0
   Set ' 100,200,300,"Hello world!", -3,-2,-1 ' Apndlist 'things'
   Let ' len list := Length(things) '

   Printnl ' "\nNormal count:\n" '
   For each( n, things, len list )
       Printnl ' "Thing : ", n '
   Next

   Printnl ' "\n\nReverse count:\n" '
   For each reverse ( n, things, len list )
       Printnl ' "Thing : ", n '
   Next
End
