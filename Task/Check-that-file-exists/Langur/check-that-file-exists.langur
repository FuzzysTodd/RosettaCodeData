val .printresult = f(.file) {
    write .file, ": "
    if val .p = prop(.file) {
        if .p'isdir {
            writeln "is directory"
        } else {
            writeln "is file"
        }
    } else {
        writeln "not found or not accessible"
    }
}

.printresult("input.txt")
.printresult("/input.txt")
.printresult("docs")
.printresult("/docs")
