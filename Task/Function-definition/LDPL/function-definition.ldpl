data:
n is number

procedure:
sub multiply
    parameters:
        x is number
        y is number
        result is number
    procedure:
        in result solve x * y
end sub

# call the bare sub-procedure
call multiply with 3 4 n
display n lf

# create a statement for it
create statement "multiply $ by $ in $" executing multiply

multiply 3 by 4 in n
display n lf
