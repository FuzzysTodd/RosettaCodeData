PROGRAM  "Factors of an integer"
VERSION  "0.0000"

DECLARE FUNCTION  Entry ()
DECLARE FUNCTION printFactors (n)

FUNCTION  Entry ()
     printFactors(11)
     printFactors(21)
     printFactors(32)
     printFactors(45)
     printFactors(67)
     printFactors(96)
END FUNCTION

FUNCTION printFactors (n)
    PRINT n; " =>";
    FOR i = 1 TO n / 2
        IF n MOD i = 0 THEN PRINT i; " ";
    NEXT i
    PRINT n
END FUNCTION
END PROGRAM
