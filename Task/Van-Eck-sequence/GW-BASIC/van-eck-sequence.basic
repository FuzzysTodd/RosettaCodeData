100 CLS
110 L = 1000
120 DIM A(L)
130 FOR N = 0 TO L
140   LET A(N) = 0
150 NEXT N
160 FOR N = 0 TO L-1
170   FOR M = N-1 TO 0 STEP -1
180     IF A(M) = A(N) THEN A(N+1) = N - M : GOTO 200
190   NEXT M
200 NEXT N
210 PRINT "Secuencia de Van Eck:"; CHR$(10)
220 PRINT "Primeros 10 terminos: ";
230 FOR I = 0 TO 9
240   PRINT A(I);
250 NEXT I
260 PRINT CHR$(10); "Terminos 991 al 1000: ";
270 FOR I = 990 TO 999
280   PRINT A(I);
290 NEXT I
300 END
