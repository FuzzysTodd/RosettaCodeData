10 LET X = 4
20 LET Y = 5
30 DIM A(3)
40 DIM B(4)
50 DIM C(8)
60 FOR I = 1 TO X
70  LET A(I-1) = I
80 NEXT I
90 FOR I = 1 TO Y
100  LET B(I-1) = I*10
110 NEXT I
120 FOR I = 1 TO X
130  LET C(I-1) = A(I-1)
140 NEXT I
150 FOR I = 1 TO Y
160  LET C(X+I-1) = B(I-1)
