PROGRAM  "IntegerArithmetic"
VERSION  "0.0000"

DECLARE FUNCTION  Entry ()

FUNCTION  Entry ()
  a$ = INLINE$("Enter integer A: ")
  a = SLONG(a$)
  b$ = INLINE$("Enter integer B: ")
  b = SLONG(b$)
  PRINT
  PRINT "       Sum:"; a + b
  PRINT "Difference:"; a - b
  PRINT "   Product:"; a * b
  PRINT "  Quontent:"; a / b
  PRINT " Remainder:"; a MOD b
  PRINT "     Power:"; a ** b
END FUNCTION
END PROGRAM
