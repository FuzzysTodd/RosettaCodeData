100 REM Largest proper divisor of n
110 PRINT "The largest proper divisor of n is:"
120 PRINT
130 PRINT USING " ## ##": 1, 1;
140 FOR I = 3 TO 100
150    FOR J = I - 1 TO 1 STEP -1
160       IF MOD(I, J) = 0 THEN
170          PRINT USING "###": J;
180          EXIT FOR
290       END IF
200    NEXT J
210    IF MOD(I, 10) = 0 THEN PRINT
220 NEXT I
230 END
