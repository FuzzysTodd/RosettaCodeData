110 DIM V(7)
120 DATA 7,6,5,4,3,2,1,0
130 FOR C = 0 TO 7
140   READ V(C)
150 NEXT C
160 DIM I(2)
170 DATA 6,1,7
180 FOR C = 0 TO 2
190   READ I(C)
200 NEXT C
210 PRINT "BEFORE SORT:"
220 FOR C = 0 TO 7
230   PRINT V(C);" ";
240 NEXT C
250 PRINT
260 PRINT
270 PRINT "AFTER SORT:"
280 FOR C = 0 TO 1
290   IF V(I(C)) > V(I(C+1)) THEN 310
300   GOTO 340
310   LET T = V(I(C))
320   LET V(I(C)) = V(I(C+1))
330   LET V(I(C+1)) = T
340 NEXT C
350 FOR C = 0 TO 7
360   PRINT V(C);" ";
370 NEXT C
380 END
