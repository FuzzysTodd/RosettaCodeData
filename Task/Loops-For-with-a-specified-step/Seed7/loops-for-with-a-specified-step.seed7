$ include "seed7_05.s7i";

const proc: main is func
  local
    var integer: number is 0;
  begin
    for number range 0 to 10 step 2 do        #  10 is inclusive
      writeln(number);
    end for;

    writeln; # spacer

    for number range 10 downto 0 step 2 do
      writeln(number);
    end for;

  end func;
