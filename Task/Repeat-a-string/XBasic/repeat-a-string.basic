PROGRAM	"Repeat a string"
VERSION	"0.0000"

DECLARE FUNCTION  Entry ()
DECLARE FUNCTION  StringRepeat$ (s$, n)

FUNCTION  Entry ()

PRINT StringRepeat$ ("rosetta", 1)
PRINT StringRepeat$ ("ha", 5)
PRINT StringRepeat$ ("*", 5)

END FUNCTION
FUNCTION StringRepeat$ (s$, n)
    cad$ = ""
    FOR i = 1 TO n
        cad$ = cad$ + s$
    NEXT i
    RETURN cad$
END FUNCTION
END PROGRAM
